function sendQr(i){"talleres"==win_name?network.postVisitWorkshop(i,function(i){0!=i&&Ti.UI.createAlertDialog({message:i.msg,ok:L("ok"),title:L("alert_title")}).show()}):"exposiciones"==win_name&&network.postVisitExposition(i,function(i){0!=i&&Ti.UI.createAlertDialog({message:i.msg,ok:L("ok"),title:L("alert_title")}).show()})}function validarQR(i){var e=i.split(":");return"emobile"==e[0]?e[1]:!1}var scanditsdk=require("com.mirasense.scanditsdk"),network=require("lib/network");("iphone"==Ti.Platform.osname||"ipad"==Ti.Platform.osname)&&(Ti.UI.iPhone.statusBarHidden=!0);var win_name=Ti.UI.currentWindow.win_name,picker,window=Ti.UI.createWindow({title:"Scandit SDK",navBarHidden:!0,width:"100%",height:"100%",layout:"composite",modal:!0,fullscreen:!1}),openScanner=function(){picker=scanditsdk.createView({width:"100%",height:"100%",zIndex:0}),picker.init("XWkcvlLgEeOZgSuLYTIqxK5VJ0cL3LOltYWM/qYAWTM",0),picker.showToolBar(!0),picker.setSuccessCallback(function(i){var e=validarQR(i.barcode);0!=e&&sendQr(e)}),picker.setCancelCallback(function(){closeScanner()}),window.add(picker),window.addEventListener("open",function(){"iphone"==Ti.Platform.osname||"ipad"==Ti.Platform.osname?picker.setOrientation(Ti.UI.orientation):picker.setOrientation(window.orientation),picker.setSize(Ti.Platform.displayCaps.platformWidth,Ti.Platform.displayCaps.platformHeight),picker.startScanning()}),window.open()},closeScanner=function(){null!=picker&&(picker.stopScanning(),window.remove(picker)),window.close(),Ti.UI.currentWindow.close()};Ti.Gesture.addEventListener("orientationchange",function(i){window.orientationModes=[Ti.UI.PORTRAIT,Ti.UI.UPSIDE_PORTRAIT,Ti.UI.LANDSCAPE_LEFT,Ti.UI.LANDSCAPE_RIGHT],null!=picker&&(picker.setOrientation(i.orientation),picker.setSize(Ti.Platform.displayCaps.platformWidth,Ti.Platform.displayCaps.platformHeight))}),openScanner();