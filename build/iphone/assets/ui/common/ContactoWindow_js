function ContactoWindow(){function e(){contWindow.open()}function o(){contWindow.close()}var t=require("lib/network"),i="92%",n="90%",r=r||{};r.top||80;var a=require("tools"),s=a.isiOS7Plus();contWindow=Ti.UI.createWindow({height:"100%",width:"100%",navBarHidden:!0,backgroundColor:"transparent",opacity:1,fullscreen:s}),blackWindow=Ti.UI.createView({height:n,width:i,borderRadius:10,backgroundColor:"#000",opacity:.9,center:{y:"50%"},layout:"vertical"}),buttonClose=Ti.UI.createImageView({id:"buttonClose",image:"/images/close.png",width:30,height:30,right:10,top:10}),imageViewIcono=Ti.UI.createImageView({id:"imageViewIcono",width:50,height:50,left:"10",image:"/images/bueno.png"}),scrollView=Ti.UI.createScrollView({width:"100%",height:"100%",backgroundColor:"transparent",layout:"vertical",bottom:20}),sugerencias=Ti.UI.createTextArea({width:"90%",height:100,top:"2%",left:"5%",hintText:L("comments"),maxLength:100}),btnEnvia=Ti.UI.createButton({title:L("send"),right:"5%",top:"2%",height:Ti.UI.SIZE,width:"25%"}),labelHeader=Ti.UI.createLabel({text:L("suggestions"),height:Ti.UI.SIZE,font:{fontSize:"18dp",fontWeight:"bold"},color:"white",top:10,center:{x:"50%"}}),labelDescripcion=Ti.UI.createLabel({text:L("joinus")+L("joinus2"),height:Ti.UI.SIZE,left:"5%",width:"90%",font:{fontSize:"14dp"},color:"white",top:"2%",textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT}),labelPregunta=Ti.UI.createLabel({text:L("rating"),height:Ti.UI.SIZE,left:"5%",width:"90%",font:{fontSize:"14dp"},color:"white",top:"2%"}),viewLinea=Ti.UI.createView({width:"90%",height:1,backgroundColor:"white",top:50,left:"5%"}),viewTop=Ti.UI.createView({width:"100%",height:Ti.UI.SIZE,backgroundColor:"transparent"}),viewRow=Ti.UI.createView({width:"100%",height:Ti.UI.SIZE,backgroundColor:"transparent",layout:"horizontal"}),viewContainer=Ti.UI.createView({width:"100%",height:Ti.UI.SIZE,backgroundColor:"transparent",layout:"vertical"}),spCalificaciones=Ti.UI.createPicker({left:"5%",top:"2%",height:"180",width:"150"});var c=[],d=L("qualifications").split(",");camposDisplay=L("joinus");var l=3;c[0]=Ti.UI.createPickerRow({value:3,title:d[0]}),c[1]=Ti.UI.createPickerRow({value:2,title:d[1]}),c[2]=Ti.UI.createPickerRow({value:1,title:d[2]});var p=d[0];spCalificaciones.add(c),spCalificaciones.selectionIndicator=!0,viewTop.add(buttonClose),viewTop.add(labelHeader),viewTop.add(viewLinea),viewRow.add(spCalificaciones),viewRow.add(imageViewIcono),viewContainer.add(labelPregunta),viewContainer.add(viewRow),viewContainer.add(labelDescripcion),viewContainer.add(sugerencias),viewContainer.add(btnEnvia),scrollView.add(viewTop),scrollView.add(viewContainer),blackWindow.add(scrollView),contWindow.add(blackWindow),contWindow.openView=e,contWindow.closeView=o,spCalificaciones.addEventListener("change",function(){l=spCalificaciones.getSelectedRow(0).value,p=spCalificaciones.getSelectedRow(0).title,3==l?imageViewIcono.image="/images/bueno.png":2==l?imageViewIcono.image="/images/regular.png":1==l&&(imageViewIcono.image="/images/malo.png")}),buttonClose.addEventListener("click",function(){Ti.Media.vibrate(),o()}),btnEnvia.addEventListener("click",function(){Ti.Media.vibrate();var e=Ti.Database.open("anadicDB");e.execute("CREATE TABLE IF NOT EXISTS rating(rateId INTEGER PRIMARY KEY, rate TEXT);"),t.postSuggestions(l,sugerencias.value,function(e){if(0!=e){var t=Ti.Database.open("anadicDB");t.execute("INSERT INTO rating VALUES (1,'"+p+"');"),t.close(),Ti.UI.createAlertDialog({message:e.msg,ok:L("ok"),title:L("alert_title")}).show(),o()}})}),contWindow.addEventListener("android:back",function(){Ti.Media.vibrate(),o()}),contWindow.open()}module.exports.ContactoWindow=ContactoWindow;