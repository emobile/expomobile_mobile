function HorariosWindow(e){function i(){var i=[];e.forEach(function(e){var t=e.split("/"),o=new Date(t[2],t[1]-1,t[0]),a=r[o.getDay()]+" "+o.getDate()+" - "+d[o.getMonth()],n=Ti.UI.createTableViewRow({id:e,title:a,isparent:!0,opened:!1,hasChild:!0,sub:[],font:{fontSize:"20dp"},color:"black"});s[""+a]=[],i.push(n)}),c.setData(i)}function t(){Ti.Media.vibrate(),l.close()}var o=require("lib/network"),a=require("ui/common/InfoWindow"),n=a.InfoWindow(),r=L("weekDays").split(","),d=L("months").split(","),l=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:!1,navBarHidden:!0}),c=Ti.UI.createTableView({width:"90%",height:Ti.Platform.displayCaps.platformHeight,style:Ti.UI.iPhone.TableViewStyle.GROUPED});scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),imageViewBar=Ti.UI.createView({id:"imageViewBar",backgroundColor:Ti.App.Properties.getString("viewcolor"),height:80,left:0,top:0,width:"100%"}),imageView=Ti.UI.createImageView({id:"imageView",image:"/images/horarios_blanco.png",width:60,height:60,top:10,left:10}),labelTitulo=Ti.UI.createLabel({id:"labelTitulo",height:"auto",text:L("activities"),font:{fontSize:"22dp"},color:"white",center:{x:"50%"},top:15}),labelSubTitulo=Ti.UI.createLabel({id:"labelSubtitulo",height:"auto",text:"Subtitulo",font:{fontSize:"18dp"},color:"white",center:{x:"50%"},top:40}),buttonClose=Ti.UI.createImageView({id:"buttonClose",image:"/images/close.png",width:30,height:30,top:10,right:10}),scrollView_1.add(c),imageViewBar.add(imageView),imageViewBar.add(buttonClose),imageViewBar.add(labelTitulo),l.add(imageViewBar),l.add(scrollView_1);var s=[],g=[];return i(),c.addEventListener("click",function(e){if(e.row.isparent)if(e.row.opened){for(var i=e.row.sub.length;i>0;i-=1)c.deleteRow(e.index+i);e.row.opened=!1}else if(0==s[e.rowData.title].length)o.getDataParam(o.SERVICES.ACTIVITIES,e.row.id,function(i){var t=i;labelsEventos=[];for(var o=0;t.length>o;o++){var a={};a.id=""+t[o].id,a.left="15%",a.title=t[o].name+" \r "+t[o].start_date.substring(11,16)+" - "+t[o].end_date.substring(11,16)+" \r "+t[o].place,a.font={fontSize:"14dp"},a.color="#424242",labelsEventos.push(a)}e.row.sub=labelsEventos;for(var n=0;labelsEventos.length>n;n++)c.insertRowAfter(e.index,labelsEventos[n],{animationStyle:Ti.UI.iPhone.RowAnimationStyle.RIGHT});s[e.rowData.title]=t,g[e.rowData.title]=labelsEventos,e.row.opened=!0});else{for(var i=0;s[e.rowData.title].length>i;i++)c.insertRowAfter(e.index,g[e.rowData.title][i],{animationStyle:Ti.UI.iPhone.RowAnimationStyle.RIGHT});e.row.opened=!0}else for(var t in s)s[t].forEach(function(i){if(e.rowData.id==i.id){var t="",o="";if(i.start_date.length>10){var a=i.start_date.substring(0,10),r=a.split("-");t=r[2]+"/"+d[r[1]-1]+"/"+r[0],o=i.start_date.substring(11,16)+" - "+i.end_date.substring(11,16)}var l={};l[L("name")]=i.name,l[L("start_date")]=t,l[L("place")]=i.place,l[L("hora")]=o,null!=i.observations&&(l[L("observations")]=i.observations),n.openView(l)}})}),buttonClose.addEventListener("click",function(){t()}),l.addEventListener("android:back",function(){t()}),l}module.exports=HorariosWindow;