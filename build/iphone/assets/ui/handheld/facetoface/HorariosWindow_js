function HorariosWindow(e,i){function t(){Ti.Media.vibrate(),faceHorWdw.close()}function o(e){var i=[];s=e;for(var t=0;e.length>t;t++){var o=e[t].split("/"),r=new Date(o[2],o[1]-1,o[0]),d=a[r.getDay()]+" "+r.getDate()+" - "+n[r.getMonth()],l=Ti.UI.createTableViewRow({id:t,title:d,leftImage:"/images/horarios.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});i.push(l)}table.setData(i)}require("lib/network");var a=L("weekDays").split(","),n=L("months").split(","),r=require("tools"),d=r.isiOS7Plus();faceHorWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:d,navBarHidden:!0}),table=Ti.UI.createTableView({width:"90%",height:"100%"}),scrollView_1=Ti.UI.createScrollView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),scrollView_1.add(table);var l=require("templates"),c=l.getTopBar(L("facetoface"),"/images/horarios_blanco.png",t);faceHorWdw.add(c),faceHorWdw.add(scrollView_1);var s;return o(i),table.addEventListener("click",function(e){var i,t=require("ui/handheld/facetoface/DetalleWindow");new t(i,s[e.index]).open()}),faceHorWdw.addEventListener("android:back",function(){Ti.Media.vibrate(),faceHorWdw.close()}),faceHorWdw}module.exports=HorariosWindow;