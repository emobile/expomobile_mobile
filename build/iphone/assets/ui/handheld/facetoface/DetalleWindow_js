function DetalleWindow(e,i){function t(){Ti.Media.vibrate(),faceDetWdw.close()}function o(){u>0?(labelCuentaCitas.text=w+1+" "+L("of")+" "+u,textFieldEntrevista.value=c[w].int_name,textFieldEmpresa.value=c[w].int_social_reason,textFieldTelefono.value=c[w].attendee_phone,textFieldFechaInicio.value=a(c[w].start_date),textFieldFechaTermino.value=a(c[w].end_date)):b.show()}function a(e){var i=e.substring(0,10).split("-"),t=e.substring(11,16).split("-"),o=i[2]+"-"+i[1]+"-"+i[0]+"  "+t;return o}var n=require("lib/network"),r="",d=Ti.Database.open("anadicDB"),l=d.execute("SELECT name FROM users WHERE userId = 1;");l.isValidRow()?(r=l.fieldByName("name"),("undefined"===r||"null"==r)&&(r="")):r="",l.close(),d.close();var c,s,u,w=0,g=require("tools"),h=g.isiOS7Plus();faceDetWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundImage:"/images/background.png",width:"100%",height:"100%",layout:"composite",fullscreen:h,navBarHidden:!0}),faceDetWdw.addEventListener("load",function(){textFieldEntrevista.blur()}),scrollView=Ti.UI.createView({id:"scrollView",backGroundColor:"transparent",height:Ti.UI.SIZE,width:"100%",layout:"vertical",top:"80"});var p=require("templates"),T=p.getTopBar(L("facetoface"),"/images/iconfacetoface.png",t);bottomBar=Ti.UI.createView({id:"bottomBar",backgroundColor:"transparent",height:Ti.UI.SIZE,width:"100%",bottom:"5"}),buttonFirst=Ti.UI.createImageView({id:"buttonFirst",image:"/images/buttonfirst.png",width:48,height:48,top:"0%",left:"10px"}),buttonLast=Ti.UI.createImageView({id:"buttonLast",image:"/images/buttonfinal.png",width:48,height:48,top:"0%",right:"10px"}),s=Ti.UI.createView({backgroundColor:"transparent",width:"100%",height:Ti.UI.SIZE,layout:"vertical"}),labelCuentaCitas=Ti.UI.createLabel({id:"labelCuentaCitas",height:"auto",width:"auto",font:{fontWeight:"bold",fontSize:"20dp"},color:"#2c2c2c",right:"5%"}),labelSocioTitulo=Ti.UI.createLabel({id:"labelSocioTitulo",height:"auto",width:"auto",text:L("partner"),font:{fontWeight:"bold",fontSize:"20dp"},