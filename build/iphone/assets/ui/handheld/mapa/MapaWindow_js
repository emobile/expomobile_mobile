function MapaWindow(){function e(){Ti.Media.vibrate(),mapaWdw.close()}var i=require("tools"),t=i.isiOS7Plus();mapaWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:t,navBarHidden:!0}),webView=Ti.UI.createWebView({id:"webView",url:"/ui/handheld/mapa/index.html",height:"100%",width:"100%",scalesPageToFit:!0});var o=require("templates"),a=o.getTopBar(L("map"),"/images/iconmapa.png",e);return mapaWdw.add(a),mapaWdw.add(webView),mapaWdw.addEventListener("android:back",function(){e()}),mapaWdw}module.exports=MapaWindow;