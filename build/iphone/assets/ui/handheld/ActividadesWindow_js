function ActividadesWindow(){function e(){Ti.Media.vibrate(),n.close()}function i(){var e=[],i=Ti.UI.createTableViewRow({id:2,title:L("schedules"),leftImage:"/images/horarios.png",isparent:!0,opened:!1,hasChild:!0,sub:[{id:"horariosexpositores",left:"15%",title:L("exhibitors"),font:{fontSize:"20dp"},color:"#424242"},{id:"horariosconferencias",left:"15%",title:L("conferences"),font:{fontSize:"20dp"},color:"#424242"}],font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:3,title:L("map"),leftImage:"/images/mapa.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i),a.setData(e)}var t=require("tools"),o=t.isiOS7Plus(),n=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:o,navBarHidden:!0}),a=Ti.UI.createTableView({width:"90%",height:"100%"});scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),scrollView_1.add(a);var r=require("templates"),c=r.getTopBar(L("activities"),"/images/iconactividades.png",e);return n.add(c),n.add(scrollView_1),i(),a.addEventListener("click",function(e){if(2==e.rowData.id){var i=require("lib/network");i.getData(i.SERVICES.ACTIVITIES_DAYS,function(e){if(0==e.length)Ti.UI.createAlertDialog({message:L("no_actividades"),ok:L("ok"),title:L("alert_title")}).show();else if(e.length>0){var i,t=require("ui/handheld/actividades/HorariosWindow");new t(e,i).open()}})}else if(3==e.rowData.id){var t,o=require("ui/handheld/mapa/MapaWindow");new o(t).open()}}),n.addEventListener("android:back",function(){Ti.Media.vibrate(),n.close()}),n}module.exports=ActividadesWindow;