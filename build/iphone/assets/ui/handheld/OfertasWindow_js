function OfertasWindow(){function e(){var e=[],i=Ti.UI.createTableViewRow({id:1,title:L("byexhibitor"),leftImage:"/images/expositor.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:2,title:L("alloffers"),leftImage:"/images/expositores.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:3,title:L("map"),leftImage:"/images/mapa.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i),table.setData(e)}return windowOfertas=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",oldWin:Ti.currentWindow,fullscreen:!1,navBarHidden:!0}),table=Ti.UI.createTableView({top:10,width:"90%",height:"100%"}),scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),scrollView_1.add(table),imageViewBar=Ti.UI.createView({id:"imageViewBar",backgroundColor:Ti.App.Properties.getString("viewcolor"),height:80,left:0,top:0,width:"100%",layout:"horizontal"}),imageView=Ti.UI.createImageView({id:"imageView",image:"/images/iconofertas.png",width:60,height:60,top:7,right:3}),imageViewBar.add(imageView),labelTitulo=Ti.UI.createLabel({id:"labelTitulo",height:"auto",width:"70%",text:L("offers"),font:{fontSize:"22dp"},color:"white",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),imageViewBar.add(labelTitulo),buttonClose=Ti.UI.createImageView({id:"buttonClose",image:"/images/close.png",width:30,height:30,top:25}),imageViewBar.add(buttonClose),windowOfertas.add(imageViewBar),windowOfertas.add(scrollView_1),e(),table.addEventListener("click",function(e){if(1==e.rowData.id){var i,t=require("ui/handheld/ofertas/OpcionesExpositorWindow");new t(i).open()}else if(2==e.rowData.id){var i,t=require("ui/handheld/ofertas/ExpositoresWindow");new t(i,0,0,0).open()}else if(3==e.rowData.id){var i,t=require("ui/handheld/mapa/MapaWindow");new t(i).open()}}),windowOfertas.addEventListener("android:back",function(){Ti.Media.vibrate(),windowOfertas.close()}),buttonClose.addEventListener("click",function(){Ti.Media.vibrate(),windowOfertas.close()}),windowOfertas}module.exports=OfertasWindow;