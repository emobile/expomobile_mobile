function RegistroWindow(){function e(){Ti.Media.vibrate(),registroWdw.close()}require("ui/common/ContactoWindow");var i=require("tools"),t=i.isiOS7Plus();registroWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"auto",layout:"vertical",fullscreen:t,navBarHidden:!0});var o="black",a="gray",n=Ti.Database.open("anadicDB"),r=n.execute("SELECT id, enterprise, address, phone, group_name, subgroup_name FROM users WHERE userId = 1;"),d=require("templates"),s=d.getTopBar(L("register"),"/images/iconregistro.png",e);return scrollView=Ti.UI.createScrollView({id:"scrollView",backgroundImage:"/images/background.png",height:"auto",width:"100%",layout:"vertical"}),labelEmpresa=Ti.UI.createLabel({id:"labelEmpresa",height:"auto",width:"70%",text:L("enterprise"),font:{fontSize:"16dp"},color:o,textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT,top:"10px"}),textEmpresa=Ti.UI.createTextArea({id:"textEmpresa",value:r.fieldByName("enterprise"),height:Ti.UI.SIZE,width:"70%",font:{fontSize:"16dp"},color:a,enabled:"false"}),labelTelefono=Ti.UI.createLabel({id:"labelTelefono",height:"auto",width:"70%",text:L("telephone"),font:{fontSize:"16dp"},color:o,textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT}),textTelefono=Ti.UI.createTextArea({id:"textTelefono",value:r.fieldByName("phone"),height:Ti.UI.SIZE,width:"70%",font:{fontSize:"16dp"},color:a,enabled:"false"}),labelDireccion=Ti.UI.createLabel({id:"labelDireccion",height:"auto",width:"70%",text:L("address"),font:{fontSize:"16dp"},color:o,textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT}),textDireccion=Ti.UI.createTextArea({id:"textDireccion",value:r.fieldByName("address"),height:Ti.UI.SIZE,width:"70%",font:{fontSize:"16dp"},color:a,enabled:"false"}),labelGrupo=Ti.UI.createLabel({id:"labelGrupo",height:"auto",width:"70%",text:L("grupo"),font:{fontSize:"16dp"},color:o,textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT}),textGrupo=Ti.UI.createTextArea({id:"textSubgrupo",value:r.fieldByName("group_name"),height:Ti.UI.SIZE,width:"70%",font:{fontSize:"16dp"},color:a,enabled:"false"}),labelSubgrupo=Ti.UI.createLabel({id:"labelSubgrupo",height:"auto",width:"70%",text:L("subgrupo"),font:{fontSize:"16dp"},color:o,textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT}),textSubgrupo=Ti.UI.createTextArea({id:"textSubgrupo",value:r.fieldByName("subgroup_name"),height:Ti.UI.SIZE,width:"70%",font:{fontSize:"16dp"},color:a,enabled:"false"}),buttonSugerencias=Ti.UI.createButton({id:"buttonSugerencias",height:"auto",width:"70%",title:L("suggestions"),font:{fontSize:"18dp"},color:"blue",top:"10px",systemButton:Ti.UI.iPhone.SystemButton.CANCEL}),n.close(),r.close(),scrollView.add(labelEmpresa),scrollView.add(textEmpresa),scrollView.add(labelTelefono),scrollView.add(textTelefono),scrollView.add(labelDireccion),scrollView.add(textDireccion),scrollView.add(labelGrupo),scrollView.add(textGrupo),scrollView.add(labelSubgrupo),scrollView.add(textSubgrupo),scrollView.add(buttonSugerencias),registroWdw.add(s),registroWdw.add(scrollView),buttonSugerencias.addEventListener("click",function(){var e=Ti.Database.open("anadicDB");e.execute("CREATE TABLE IF NOT EXISTS rating(rateId INTEGER PRIMARY KEY, rate TEXT);");var i=e.execute("SELECT rate FROM rating WHERE rateId = 1;");if(i.isValidRow())Ti.UI.createAlertDialog({message:L("qualified")+"\n"+L("qualified2")+" "+i.fieldByName("rate"),ok:L("ok"),title:L("alert_title")}).show();else{var t=require("/ui/common/ContactoWindow");new t.ContactoWindow}e.close(),i.close()}),registroWdw.addEventListener("android:back",function(){Ti.Media.vibrate(),registroWdw.close()}),registroWdw}module.exports=RegistroWindow;