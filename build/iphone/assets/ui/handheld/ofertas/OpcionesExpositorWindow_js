function OpcionesExpositorWindow(){function e(){Ti.Media.vibrate(),windowOpc.close()}function i(){for(var e=[],i=Ti.Database.open("anadicDB"),t=i.execute("SELECT * FROM exhibitors");t.isValidRow();){var o,a="http://"+Ti.App.Properties.getString("hostname")+t.fieldByName("mobile_logo_url"),n=/missing.jpg/g,r=n.test(a);o=r?"/images/unavailable_small.jpg":"http://"+Ti.App.Properties.getString("hostname")+t.fieldByName("mobile_logo_url"),e.push({id:t.fieldByName("id"),title:t.fieldByName("name"),leftImage:o,font:{fontSize:"22dp"},color:"black"}),table.setData(e),t.next()}t.close(),i.close()}var t=require("tools"),o=t.isiOS7Plus();windowOpc=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:o,navBarHidden:!0}),table=Ti.UI.createTableView({width:"90%",height:"100%"}),scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),scrollView_1.add(table);var a=require("templates"),n=a.getTopBar(L("offers"),"/images/iconofertas.png",e);return windowOpc.add(n),windowOpc.add(scrollView_1),i(),table.addEventListener("click",function(e){var i,t=e.rowData.id,o=e.rowData.title,a=e.rowData.leftImage,n=require("ui/handheld/ofertas/ExpositoresWindow");new n(i,t,o,a).open()}),windowOpc.addEventListener("android:back",function(){Ti.Media.vibrate(),windowOpc.close()}),buttonClose.addEventListener("click",function(){Ti.Media.vibrate(),windowOpc.close()}),windowOpc}module.exports=OpcionesExpositorWindow;