function HorariosWindow(e){function i(){Ti.Media.vibrate(),c.close()}function t(){var i=[];e.forEach(function(e){var t=e.split("/"),o=new Date(t[2],t[1]-1,t[0]),a=s[o.getDay()]+" "+o.getDate()+" - "+l[o.getMonth()],n=Ti.UI.createTableViewRow({id:e,title:a,isparent:!0,opened:!1,hasChild:!0,sub:[],font:{fontSize:"20dp"},color:"black"});g[""+a]=[],i.push(n)}),u.setData(i)}var o=require("lib/network"),a=require("ui/common/InfoWindow"),n=a.InfoWindow(),r=require("tools"),d=r.isiOS7Plus(),s=L("weekDays").split(","),l=L("months").split(","),c=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:d,navBarHidden:!0}),u=Ti.UI.createTableView({width:"90%",height:Ti.Platform.displayCaps.platformHeight,style:Ti.UI.iPhone.TableViewStyle.GROUPED});scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),scrollView_1.add(u);var w=require("templates"),p=w.getTopBar(L("conferences"),"/images/horarios_blanco.png",i);c.add(p),c.add(scrollView_1);var g=[],h=[];return t(),u.addEventListener("click",function(e){if(e.row.isparent)if(e.row.opened){for(var i=e.row.sub.length;i>0;i-=1)u.deleteRow(e.index+i);e.row.opened=!1}else if(0==g[e.rowData.title].length)o.getDataParam(o.SERVICES.CONFERENCES,e.row.id,function(i){var t=i;labelsEventos=[];for(var o=0;t.length>o;o++){var a={};a.id=""+t[o].id,a.left="15%",a.title=t[o].name+" "+t[o].conferencist+" - "+t[o].place+" - "+t[o].start_date.substring(11,16)+" - "+t[o].end_date.substring(11,16)+" \r ",a.font={fontSize:"16dp"},a.color="#424242",labelsEventos.push(a)}e.row.sub=labelsEventos;for(var n=0;labelsEventos.length>n;n++)u.insertRowAfter(e.index,labelsEventos[n],{animationStyle:Ti.UI.iPhone.RowAnimationStyle.RIGHT});g[e.rowData.title]=t,h[e.rowData.title]=labelsEventos,e.row.opened=!0});else{for(var i=0;g[e.rowData.title].length>i;i++)u.insertRowAfter(e.index,h[e.rowData.title][i],{animationStyle:Ti.UI.iPhone.RowAnimationStyle.RIGHT});e.row.opened=!0}else for(var t in g)g[t].forEach(function(i){if(e.rowData.id==i.id){var t="",o="";if(i.start_date.length>10){var a=i.start_date.substring(0,10),r=a.split("-");t=r[2]+"/"+l[r[1]-1]+"/"+r[0],o=i.start_date.substring(11,16)+" - "+i.end_date.substring(11,16)}var d={};d[L("name")]=i.name,d[L("start_date")]=t,d[L("conferencist")]=i.conferencist,d[L("place")]=i.place,d[L("hora")]=o,null!=i.observations&&(d[L("observations")]=i.observations),n.openView(d)}})}),c.addEventListener("android:back",function(){i()}),c}module.exports=HorariosWindow;