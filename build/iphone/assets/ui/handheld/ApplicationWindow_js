function ApplicationWindow(){Ti.App.Properties.setString("viewcolor","#34495e");var e=require("ui/common/RegisterView"),i=require("ui/common/LoginView"),t=require("tools"),o=t.isiOS7Plus(),n=Ti.UI.createWindow({backgroundColor:"#FFFFFF",fullscreen:o,navBarHidden:!0}),a=Ti.Database.open("anadicDB");a.execute("CREATE TABLE IF NOT EXISTS users(userId INTEGER PRIMARY KEY, id TEXT, name TEXT, group_name TEXT, subgroup_name TEXT, subgroup_leader TEXT, enterprise TEXT, phone TEXT, address TEXT);");var r=a.execute("SELECT id FROM users WHERE userId = 1;");if(a.close(),r.isValidRow()){var s=new i;n.add(s)}else{var c=new e;n.add(c)}var d=Ti.UI.createAlertDialog({title:L("tittlealert"),message:L("closeapp"),buttonNames:[L("yes"),L("no")]});return d.addEventListener("click",function(e){if(0==e.index){if(Ti.Media.vibrate(),"android"==Ti.Platform.osname){var i=Ti.Android.currentActivity;i.finish()}else n.close();exit()}}),n.addEventListener("android:back",function(){d.show()}),n}module.exports=ApplicationWindow;