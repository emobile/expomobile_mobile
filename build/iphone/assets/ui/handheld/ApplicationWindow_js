function ApplicationWindow(){Ti.App.Properties.setString("viewcolor","#34495e");var e=require("ui/common/RegisterView"),i=require("ui/common/LoginView"),t=Ti.UI.createWindow({backgroundColor:"#FFFFFF"}),o=Ti.Database.open("anadicDB");o.execute("CREATE TABLE IF NOT EXISTS users(userId INTEGER PRIMARY KEY, id TEXT, name TEXT, group_name TEXT, subgroup_name TEXT, subgroup_leader TEXT, enterprise TEXT, phone TEXT, address TEXT);");var n=o.execute("SELECT id FROM users WHERE userId = 1;");if(o.close(),n.isValidRow()){var a=new i;t.add(a)}else{var r=new e;t.add(r)}var c=Ti.UI.createAlertDialog({title:L("tittlealert"),message:L("closeapp"),buttonNames:[L("yes"),L("no")]});return c.addEventListener("click",function(e){if(0==e.index){if(Ti.Media.vibrate(),"android"==Ti.Platform.osname){var i=Ti.Android.currentActivity;i.finish()}else t.close();exit()}}),t.addEventListener("android:back",function(){c.show()}),t}module.exports=ApplicationWindow;