function HorariosWindow(e,i){function t(e){var i=[];n=e;for(var t=0;e.length>t;t++){var r=e[t].split("/"),d=new Date(r[2],r[1]-1,r[0]),l=o[d.getDay()]+" "+d.getDate()+" - "+a[d.getMonth()],c=Ti.UI.createTableViewRow({id:t,title:l,leftImage:"/images/horarios.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});i.push(c)}table.setData(i)}require("lib/network");var o=L("weekDays").split(","),a=L("months").split(",");expHorWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:!1,navBarHidden:!0}),table=Ti.UI.createTableView({width:"90%",height:"100%"}),scrollView_1=Ti.UI.createScrollView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),scrollView_1.add(table),imageViewBar=Ti.UI.createView({id:"imageViewBar",backgroundColor:Ti.App.Properties.getString("viewcolor"),height:80,left:0,top:0,width:"100%",layout:"horizontal"}),imageView=Ti.UI.createImageView({id:"imageView",image:"/images/iconexposiciones.png",width:60,height:60,top:7,right:3}),imageViewBar.add(imageView),labelTitulo=Ti.UI.createLabel({id:"labelTitulo",height:"auto",width:"70%",text:L("exhibitions"),font:{fontSize:"22dp"},color:"white",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),imageViewBar.add(labelTitulo),buttonClose=Ti.UI.createImageView({id:"buttonClose",image:"/images/close.png",width:30,height:30,top:25}),imageViewBar.add(buttonClose),expHorWdw.add(imageViewBar),expHorWdw.add(scrollView_1);var n;return t(i),table.addEventListener("click",function(e){var i,t=require("ui/handheld/exposiciones/DetalleWindow");new t(i,n[e.index]).open()}),buttonClose.addEventListener("click",function(){Ti.Media.vibrate(),expHorWdw.close()}),expHorWdw.addEventListener("android:back",function(){Ti.Media.vibrate(),expHorWdw.close()}),expHorWdw}module.exports=HorariosWindow;