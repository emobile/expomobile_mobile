function HorariosWindow(e,i){function t(){Ti.Media.vibrate(),expHorWdw.close()}function o(e){var i=[];s=e;for(var t=0;e.length>t;t++){var o=e[t].split("/"),a=new Date(o[2],o[1]-1,o[0]),n=r[a.getDay()]+" "+a.getDate()+" - "+d[a.getMonth()],l=Ti.UI.createTableViewRow({id:t,title:n,leftImage:"/images/horarios.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});i.push(l)}table.setData(i)}require("lib/network");var a=require("tools"),n=a.isiOS7Plus(),r=L("weekDays").split(","),d=L("months").split(",");expHorWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:n,navBarHidden:!0}),table=Ti.UI.createTableView({width:"90%",height:"100%"}),scrollView_1=Ti.UI.createScrollView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),scrollView_1.add(table);var l=require("templates"),c=l.getTopBar(L("exhibitions"),"/images/horarios_blanco.png",t);expHorWdw.add(c),expHorWdw.add(scrollView_1);var s;return o(i),table.addEventListener("click",function(e){var i,t=require("ui/handheld/exposiciones/DetalleWindow");new t(i,s[e.index]).open()}),expHorWdw.addEventListener("android:back",function(){t()}),expHorWdw}module.exports=HorariosWindow;