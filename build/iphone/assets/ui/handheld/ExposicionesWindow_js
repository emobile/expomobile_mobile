function ExposicionesWindow(){function e(){var e=[],i=Ti.UI.createTableViewRow({id:1,title:L("directory"),leftImage:"/images/directorio.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:2,title:L("schedules"),leftImage:"/images/horarios.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:3,title:L("map"),leftImage:"/images/mapa.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:4,title:L("register_visit"),leftImage:"/images/miqrcode.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i),table.setData(e)}var i=require("lib/network");return expoWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:!1,navBarHidden:!0}),table=Ti.UI.createTableView({width:"90%",height:"100%"}),scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),scrollView_1.add(table),imageViewBar=Ti.UI.createView({id:"imageViewBar",backgroundColor:Ti.App.Properties.getString("viewcolor"),height:80,left:0,top:0,width:"100%",layout:"horizontal"}),imageView=Ti.UI.createImageView({id:"imageView",image:"/images/iconexposiciones.png",width:60,height:60,top:7,right:3}),imageViewBar.add(imageView),labelTitulo=Ti.UI.createLabel({id:"labelTitulo",height:"auto",width:"70%",text:L("exhibitions"),font:{fontSize:"22dp"},color:"white",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),imageViewBar.add(labelTitulo),buttonClose=Ti.UI.createImageView({id:"buttonClose",image:"/images/close.png",width:30,height:30,top:25}),imageViewBar.add(buttonClose),expoWdw.add(imageViewBar),expoWdw.add(scrollView_1),e(),table.addEventListener("click",function(e){if(1==e.rowData.id)i.getExhibitors(!1,function(e){if(0!=e){var i,t=require("ui/handheld/exposiciones/DirExposWindow");new t(i).open()}});else if(2==e.rowData.id)i.getExpositionDays(function(e){if(0!=e){var i,t=require("ui/handheld/exposiciones/HorariosWindow");new t(i,e).open()}});else if(3==e.rowData.id){var t,o=require("ui/handheld/mapa/MapaWindow");new o(t).open()}else if(4==e.rowData.id)if("iphone"==Ti.Platform.osname||"ipad"==Ti.Platform.osname){var n=Ti.UI.createWindow({url:"ui/handheld/QRReaderIOSWindow.js",win_name:"exposiciones"});n.open()}else{var t,o=require("ui/handheld/QrReaderWindow");new o(t,"exposiciones").open()}}),buttonClose.addEventListener("click",function(){Ti.Media.vibrate(),expoWdw.close()}),expoWdw.addEventListener("android:back",function(){Ti.Media.vibrate(),expoWdw.close()}),expoWdw}module.exports=ExposicionesWindow;