function ConferenciasWindow(){function e(){var e=[],i=Ti.UI.createTableViewRow({id:2,title:L("schedules"),leftImage:"/images/horarios.png",isparent:!0,opened:!1,hasChild:!1,sub:[{title:"Child 1"}],font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:3,title:L("map"),leftImage:"/images/mapa.png",isparent:!0,opened:!1,hasChild:!1,sub:[{title:"Child 1"}],font:{fontSize:"22dp"},color:"black"});e.push(i),table.setData(e)}return confWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:!1,navBarHidden:!0}),table=Ti.UI.createTableView({width:"90%",height:"100%"}),scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),scrollView_1.add(table),imageViewBar=Ti.UI.createView({id:"imageViewBar",backgroundColor:Ti.App.Properties.getString("viewcolor"),height:80,left:0,top:0,width:"100%",layout:"horizontal"}),imageView=Ti.UI.createImageView({id:"imageView",image:"/images/iconconferencias.png",width:60,height:60,top:7,right:3}),imageViewBar.add(imageView),labelTitulo=Ti.UI.createLabel({id:"labelTitulo",height:"auto",width:"70%",text:L("conferences"),font:{fontSize:"22dp"},color:"white",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),imageViewBar.add(labelTitulo),buttonClose=Ti.UI.createImageView({id:"buttonClose",image:"/images/close.png",width:30,height:30,top:25}),imageViewBar.add(buttonClose),confWdw.add(imageViewBar),confWdw.add(scrollView_1),e(),table.addEventListener("click",function(e){if(2==e.rowData.id){var i=require("lib/network");i.getData(i.SERVICES.CONFERENCES_DAYS,function(e){if(0==e.length)Ti.UI.createAlertDialog({message:L("no_conferencias"),ok:L("ok"),title:L("alert_title")}).show();else if(e.length>0){var i,t=require("ui/handheld/conferencias/HorariosWindow");new t(e,i).open()}})}else if(3==e.rowData.id){var t,o=require("ui/handheld/mapa/MapaWindow");new o(t).open()}}),buttonClose.addEventListener("click",function(){Ti.Media.vibrate(),confWdw.close()}),confWdw.addEventListener("android:back",function(){Ti.Media.vibrate(),confWdw.close()}),confWdw}module.exports=ConferenciasWindow;