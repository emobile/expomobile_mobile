function TalleresWindow(){function e(){Ti.Media.vibrate(),talleresWdw.close()}function i(){var e=[],i=Ti.UI.createTableViewRow({id:1,title:L("my_schedule"),leftImage:"/images/horarios.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:2,title:L("map"),leftImage:"/images/mapa.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:3,title:L("register_asistence"),leftImage:"/images/miqrcode.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i),table.setData(e)}var t=require("tools"),o=t.isiOS7Plus();talleresWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:o,navBarHidden:!0}),table=Ti.UI.createTableView({width:"90%",height:"100%"}),scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),scrollView_1.add(table);var a=require("templates"),n=a.getTopBar(L("workshops"),"/images/icontalleres.png",e);return talleresWdw.add(n),talleresWdw.add(scrollView_1),i(),table.addEventListener("click",function(e){if(1==e.rowData.id){var i=require("lib/network");i.getData(i.SERVICES.WORKSHOPS_DAYS,function(e){if(0==e.length)Ti.UI.createAlertDialog({message:L("no_workshops"),ok:L("ok"),title:L("alert_title")}).show();else if(e.length>0){var i,t=require("ui/handheld/talleres/HorariosWindow");new t(e,i).open()}})}else if(2==e.rowData.id){var t,o=require("ui/handheld/mapa/MapaWindow");new o(t).open()}else if(3==e.rowData.id)if("iphone"==Ti.Platform.osname||"ipad"==Ti.Platform.osname){var a=Ti.UI.createWindow({url:"ui/handheld/QRReaderIOSWindow.js",win_name:"talleres"});a.open()}else{var t,o=require("ui/handheld/QrReaderWindow");new o(t,"talleres").open()}}),talleresWdw.addEventListener("android:back",function(){Ti.Media.vibrate(),talleresWdw.close()}),talleresWdw}module.exports=TalleresWindow;