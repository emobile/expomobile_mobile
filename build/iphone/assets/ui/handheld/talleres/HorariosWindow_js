function HorariosWindow(e){function i(){var i=[];e.forEach(function(e){var t=e.split("/"),o=new Date(t[2],t[1]-1,t[0]),a=c[o.getDay()]+" "+o.getDate()+" - "+s[o.getMonth()],n=Ti.UI.createTableViewRow({id:e,title:a,isparent:!0,opened:!1,hasChild:!0,sub:[],font:{fontSize:"20dp"},color:"black"});h[""+a]=[],i.push(n)}),g.setData(i)}function t(){Ti.Media.vibrate(),w.close()}var o=require("lib/network"),a="",n=Ti.Database.open("anadicDB"),r=n.execute("SELECT subgroup_name FROM users WHERE userId = 1;");r.isValidRow()?(a=r.fieldByName("subgroup_name"),r.close(),("undefined"===a||"null"==a)&&(a="")):a="",n.close();var l=require("ui/common/InfoWindow"),d=l.InfoWindow(),c=L("weekDays").split(","),s=L("months").split(","),w=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:!1,navBarHidden:!0}),g=Ti.UI.createTableView({width:"90%",height:Ti.Platform.displayCaps.platformHeight,style:Ti.UI.iPhone.TableViewStyle.GROUPED});scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),imageViewBar=Ti.UI.createView({id:"imageViewBar",backgroundColor:Ti.App.Properties.getString("viewcolor"),height:80,left:0,top:0,width:"100%"}),imageView=Ti.UI.createImageView({id:"imageView",image:"/images/horarios_blanco.png",width:60,height:60,top:10,left:10}),labelTitulo=Ti.UI.createLabel({id:"labelTitulo",height:"auto",text:L("my_schedule"),font:{fontSize:"22dp"},color:"white",center:{x:"50%"},top:15}),labelSubTitulo=Ti.UI.createLabel({id:"labelSubtitulo",height:"auto",text:a,font:{fontSize:"18dp"},color:"white",center:{x:"50%"},top:40}),buttonClose=Ti.UI.createImageView({id:"buttonClose",image:"/images/close.png",width:30,height:30,top:10,right:10}),scrollView_1.add(g),imageViewBar.add(imageView),imageViewBar.add(buttonClose),imageViewBar.add(labelTitulo),imageViewBar.add(labelSubTitulo),w.add(imageViewBar),w.add(scrollView_1);var e,h=[],u=[];return i(),g.addEventListener("click",function(e){if(e.row.isparent)if(e.row.opened){for(var i=e.row.sub.length;i>0;i-=1)g.deleteRow(e.index+i);e.row.opened=!1}else if(0==h[e.rowData.title].length)o.getDataParam(o.SERVICES.WORKSHOPS,e.row.id,function(i){var t=i;labelsEventos=[];for(var o=0;t.length>o;o++){var a={};a.id=""+t[o].id,a.left="15%",a.title=t[o].name+" "+t[o].start_hour+" - "+t[o].end_hour,a.font={fontSize:"16dp"},a.color="#424242",labelsEventos.push(a)}e.row.sub=labelsEventos;for(var n=0;labelsEventos.length>n;n++)g.insertRowAfter(e.index,labelsEventos[n],{animationStyle:Ti.UI.iPhone.RowAnimationStyle.RIGHT});h[e.rowData.title]=t,u[e.rowData.title]=labelsEventos,e.row.opened=!0});else{for(var i=0;h[e.rowData.title].length>i;i++)g.insertRowAfter(e.index,u[e.rowData.title][i],{animationStyle:Ti.UI.iPhone.RowAnimationStyle.RIGHT});e.row.opened=!0}else for(var t in h)h[t].forEach(function(i){if(e.rowData.id==i.id){var t="";if(i.start_date.length>10){var o=i.start_date.substring(0,10),a=o.split("-");t=a[2]+"/"+s[a[1]-1]+"/"+a[0]}var n={};n[L("name")]=i.name,n[L("start_date")]=t,n[L("start_hour")]=i.start_hour,n[L("end_hour")]=i.end_hour,n[L("teacher")]=i.teacher_name,n[L("room_id")]=i.room_id,d.openView(n)}})}),buttonClose.addEventListener("click",function(){t()}),w.addEventListener("android:back",function(){t()}),w}module.exports=HorariosWindow;