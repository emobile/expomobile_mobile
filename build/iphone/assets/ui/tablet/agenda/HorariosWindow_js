function HorariosWindow(e){function i(){Ti.Media.vibrate(),w.close()}function t(){var i=[];e.forEach(function(e){var t=e.split("/"),o=new Date(t[2],t[1]-1,t[0]),a=c[o.getDay()]+" "+o.getDate()+" - "+s[o.getMonth()],n=Ti.UI.createTableViewRow({id:e,title:a,isparent:!0,opened:!1,hasChild:!0,sub:[],font:{fontSize:"20dp"},color:"black"});p[""+a]=[],i.push(n)}),u.setData(i)}function o(){Ti.Media.vibrate(),w.close()}var a=require("lib/network"),n=require("tools"),r=n.isiOS7Plus(),l=require("ui/common/InfoWindow"),d=l.InfoWindow(),c=L("weekDays").split(","),s=L("months").split(","),w=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:r,navBarHidden:!0}),u=Ti.UI.createTableView({width:"90%",height:Ti.Platform.displayCaps.platformHeight,style:Ti.UI.iPhone.TableViewStyle.GROUPED});scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"});var g=require("templates"),h=g.getTopBar(L("diary"),"/images/horarios_blanco.png",i);scrollView_1.add(u),w.add(h),w.add(scrollView_1);var e,p=[],b=[];return t(),u.addEventListener("click",function(e){if(e.row.isparent)if(e.row.opened){for(var i=e.row.sub.length;i>0;i-=1)u.deleteRow(e.index+i);e.row.opened=!1}else if(0==p[e.rowData.title].length)a.getDataParam(a.SERVICES.DIARIES,e.row.id,function(i){var t=i;labelsEventos=[];for(var o=0;t.length>o;o++){var a={};a.id=""+t[o].id,a.left="15%",a.title=t[o].description+" - "+t[o].place+" - "+t[o].event_date.substring(11,16)+" - "+t[o].event_end_date.substring(11,16),a.font={fontSize:"14dp"},a.color="#424242",labelsEventos.push(a)}e.row.sub=labelsEventos;for(var n=0;labelsEventos.length>n;n++)u.insertRowAfter(e.index,labelsEventos[n],{animationStyle:Ti.UI.iPhone.RowAnimationStyle.RIGHT});p[e.rowData.title]=t,b[e.rowData.title]=labelsEventos,e.row.opened=!0});else{for(var i=0;p[e.rowData.title].length>i;i++)u.insertRowAfter(e.index,b[e.rowData.title][i],{animationStyle:Ti.UI.iPhone.RowAnimationStyle.RIGHT});e.row.opened=!0}else for(var t in p)p[t].forEach(function(i){if(e.rowData.id==i.id){var t="",o="";if(i.event_date.length>10){var a=i.event_date.substring(0,10),n=a.split("-");t=n[2]+"/"+s[n[1]-1]+"/"+n[0],o=i.event_date.substring(11,16)+" - "+i.event_end_date.substring(11,16)}var r={};r[L("name")]=i.description,r[L("start_date")]=t,r[L("event_type")]=i.event_type,r[L("place")]=i.place,r[L("hora")]=o,null!=i.observations&&(r[L("observations")]=i.observations),d.openView(r)}})}),w.addEventListener("android:back",function(){o()}),w}module.exports=HorariosWindow;