function ExposicionesWindow(){function e(){Ti.Media.vibrate(),expoWdw.close()}function i(){var e=[],i=Ti.UI.createTableViewRow({id:1,title:L("directory"),leftImage:"/images/directorio.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:2,title:L("schedules"),leftImage:"/images/horarios.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:3,title:L("map"),leftImage:"/images/mapa.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:4,title:L("register_visit"),leftImage:"/images/miqrcode.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i),table.setData(e)}var t=require("lib/network"),o=require("tools"),a=o.isiOS7Plus();expoWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:a,navBarHidden:!0}),table=Ti.UI.createTableView({width:"90%",height:"100%"}),scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"});var n=require("templates"),r=n.getTopBar(L("exhibitions"),"/images/iconexposiciones.png",e);return scrollView_1.add(table),expoWdw.add(r),expoWdw.add(scrollView_1),i(),table.addEventListener("click",function(e){if(1==e.rowData.id)t.getExhibitors(!1,function(e){if(0!=e){var i,t=require("ui/handheld/exposiciones/DirExposWindow");new t(i).open()}});else if(2==e.rowData.id)t.getExpositionDays(function(e){if(0!=e){var i,t=require("ui/handheld/exposiciones/HorariosWindow");new t(i,e).open()}});else if(3==e.rowData.id){var i,o=require("ui/handheld/mapa/MapaWindow");new o(i).open()}else if(4==e.rowData.id)if("iphone"==Ti.Platform.osname||"ipad"==Ti.Platform.osname){var a=Ti.UI.createWindow({url:"ui/handheld/QRReaderIOSWindow.js",win_name:"exposiciones"});a.open()}else{var i,o=require("ui/