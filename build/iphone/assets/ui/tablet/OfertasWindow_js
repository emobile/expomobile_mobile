function OfertasWindow(){function e(){Ti.Media.vibrate(),windowOfertas.close()}function i(){var e=[],i=Ti.UI.createTableViewRow({id:1,title:L("byexhibitor"),leftImage:"/images/expositor.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:2,title:L("alloffers"),leftImage:"/images/expositores.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:3,title:L("map"),leftImage:"/images/mapa.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i),table.setData(e)}var t=require("tools"),o=t.isiOS7Plus();windowOfertas=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",oldWin:Ti.currentWindow,fullscreen:o,navBarHidden:!0}),table=Ti.UI.createTableView({top:10,width:"90%",height:"100%"}),scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),scrollView_1.add(table);var a=require("templates"),n=a.getTopBar(L("offers"),"/images/iconofertas.png",e);return windowOfertas.add(n),windowOfertas.add(scrollView_1),i(),table.addEventListener("click",function(e){if(1==e.rowData.id){var i,t=require("ui/handheld/ofertas/OpcionesExpositorWindow");new t(i).open()}else if(2==e.rowData.id){var i,t=require("ui/handheld/ofertas/ExpositoresWindow");new t(i,0,0,0).open()}else if(3==e.rowData.id){var i,t=require("ui/handheld/mapa/MapaWindow");new t(i).open()}}),windowOfertas.addEventListener("android:back",evento=function(i){i.source.removeEventListener("android:back",arguments.callee),e()}),windowOfertas}module.exports=OfertasWindow;