function DirectorioWindow(){function e(){i.getAllExpositions(function(e){var i=[];if(0!=e){var t=0;e.forEach(function(e){var o=Ti.UI.createTableViewRow({id:t,title:e.name,font:{fontSize:"22dp"},color:"black"});i.push(o),t++}),table.setData(i)}})}var i=require("lib/network"),t=require("tools"),o=t.isiOS7Plus();return ExpDirWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:o,navBarHidden:!0}),table=Ti.UI.createTableView({width:"90%",height:"100%"}),scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),imageViewBar=Ti.UI.createView({id:"imageViewBar",backgroundColor:Ti.App.Properties.getString("viewcolor"),height:80,left:0,top:0,width:"100%",layout:"horizontal"}),imageView=Ti.UI.createImageView({id:"imageView",image:"/images/iconexposiciones.png",width:60,height:60,top:7,right:3}),imageViewBar.add(imageView),labelTitulo=Ti.UI.createLabel({id:"labelTitulo",height:"auto",width:"70%",text:L("exhibitions"),font:{fontSize:"22dp"},color:"white",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),imageViewBar.add(labelTitulo),buttonClose=Ti.UI.createImageView({id:"buttonClose",image:"/images/close.png",width:30,height:30,top:25}),imageViewBar.add(buttonClose),ExpDirWdw.add(imageViewBar),scrollView_1.add(table),ExpDirWdw.add(scrollView_1),e(),buttonClose.addEventListener("click",function(){Ti.Media.vibrate(),ExpDirWdw.close()}),ExpDirWdw.addEventListener("android:back",function(){Ti.Media.vibrate(),ExpDirWdw.close()}),ExpDirWdw}module.exports=DirectorioWindow;