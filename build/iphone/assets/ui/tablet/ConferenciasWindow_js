function ConferenciasWindow(){function e(){Ti.Media.vibrate(),confWdw.close()}function i(){var e=[],i=Ti.UI.createTableViewRow({id:2,title:L("schedules"),leftImage:"/images/horarios.png",isparent:!0,opened:!1,hasChild:!1,sub:[{title:"Child 1"}],font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:3,title:L("map"),leftImage:"/images/mapa.png",isparent:!0,opened:!1,hasChild:!1,sub:[{title:"Child 1"}],font:{fontSize:"22dp"},color:"black"});e.push(i),table.setData(e)}var t=require("tools"),o=t.isiOS7Plus();confWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:o,navBarHidden:!0}),table=Ti.UI.createTableView({width:"90%",height:"100%"}),scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),scrollView_1.add(table);var a=require("templates"),n=a.getTopBar(L("conferences"),"/images/iconconferencias.png",e);return confWdw.add(n),confWdw.add(scrollView_1),i(),table.addEventListener("click",function(e){if(2==e.rowData.id){var i=require("lib/network");i.getData(i.SERVICES.CONFERENCES_DAYS,function(e){if(0==e.length)Ti.UI.createAlertDialog({message:L("no_conferencias"),ok:L("ok"),title:L("alert_title")}).show();else if(e.length>0){var i,t=require("ui/handheld/conferencias/HorariosWindow");new t(e,i).open()}})}else if(3==e.rowData.id){var t,o=require("ui/handheld/mapa/MapaWindow");new o(t).open()}}),confWdw.addEventListener("android:back",function(){Ti.Media.vibrate(),confWdw.close()}),confWdw}module.exports=ConferenciasWindow;