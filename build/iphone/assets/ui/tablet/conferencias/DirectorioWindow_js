function DirectorioWindow(){function e(){}require("lib/network");var i=require("tools"),t=i.isiOS7Plus(),o=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:t}),a=Ti.UI.createTableView({width:"90%",height:"100%"}),n=function(){l(),o.add(imageViewBar),o.add(scrollView_1)},r=function(){scrollView_1=Ti.UI.createScrollView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"})},l=function(){r(),scrollView_1.add(a),d()},d=function(){imageViewBar=Ti.UI.createView({id:"imageViewBar",backgroundColor:Ti.App.Properties.getString("viewcolor"),height:80,left:0,top:0,width:"100%",layout:"horizontal"}),imageView=Ti.UI.createImageView({id:"imageView",image:"/images/iconconferencias.png",width:60,height:60,top:7,right:3}),imageViewBar.add(imageView),labelTitulo=Ti.UI.createLabel({id:"labelTitulo",height:"auto",width:"70%",text:L("conferences"),font:{fontSize:"22dp"},color:"white",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),imageViewBar.add(labelTitulo),buttonClose=Ti.UI.createImageView({id:"buttonClose",image:"/images/close.png",width:30,height:30,top:25}),imageViewBar.add(buttonClose)};return e(),n(),buttonClose.addEventListener("click",function(){Ti.Media.vibrate(),o.close()}),o.addEventListener("android:back",function(){Ti.Media.vibrate(),o.close()}),o}module.exports=DirectorioWindow;