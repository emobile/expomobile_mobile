function ApplicationWindow(){Ti.App.Properties.setString("viewcolor","#34495e");var e=require("ui/common/RegisterView"),i=require("ui/common/LoginView"),t=require("tools"),o=t.isiOS7Plus(),a=Ti.UI.createWindow({backgroundColor:"#FFFFFF",fullscreen:o,navBarHidden:!0}),n=Ti.Database.open("anadicDB");n.execute("CREATE TABLE IF NOT EXISTS users(userId INTEGER PRIMARY KEY, id TEXT, name TEXT, group_name TEXT, subgroup_name TEXT, subgroup_leader TEXT, enterprise TEXT, phone TEXT, address TEXT);");var r=n.execute("SELECT id FROM users WHERE userId = 1;");if(n.close(),r.isValidRow()){var l=new i;a.add(l)}else{var d=new e;a.add(d)}var c=Ti.UI.createAlertDialog({title:L("tittlealert"),message:L("closeapp"),buttonNames:[L("yes"),L("no")]});return c.addEventListener("click",function(e){if(0==e.index){if(Ti.Media.vibrate(),"android"==Ti.Platform.osname){var i=Ti.Android.currentActivity;i.finish()}else a.close();exit()}}),a.addEventListener("android:back",function(){c.show()}),a}module.exports=ApplicationWindow;