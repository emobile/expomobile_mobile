function RegistroWindow(){require("ui/common/ContactoWindow"),registroWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"auto",layout:"vertical",fullscreen:!1,navBarHidden:!0});var e="black",i="gray",t=Ti.Database.open("anadicDB"),a=t.execute("SELECT id, enterprise, address, phone, group_name, subgroup_name FROM users WHERE userId = 1;");return scrollView=Ti.UI.createScrollView({id:"scrollView",backgroundImage:"/images/background.png",height:"auto",width:"100%",layout:"vertical"}),imageViewBar=Ti.UI.createView({id:"imageViewBar",backgroundColor:Ti.App.Properties.getString("viewcolor"),height:80,left:0,top:0,width:"100%",layout:"horizontal"}),imageView=Ti.UI.createImageView({id:"imageView",image:"/images/iconregistro.png",width:60,height:60,top:7,right:3}),labelTitulo=Ti.UI.createLabel({id:"labelTitulo",height:"auto",width:"70%",text:L("register"),font:{fontSize:"22dp"},color:"white",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),buttonClose=Ti.UI.createImageView({id:"buttonClose",image:"/images/close.png",width:30,height:30,top:25}),labelEmpresa=Ti.UI.createLabel({id:"labelEmpresa",height:"auto",width:"70%",text:L("enterprise"),font:{fontSize:"16dp"},color:e,textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT,top:"10px"}),textEmpresa=Ti.UI.createTextArea({id:"textEmpresa",value:a.fieldByName("enterprise"),height:Ti.UI.SIZE,width:"70%",font:{fontSize:"16dp"},color:i,enabled:"false"}),labelTelefono=Ti.UI.createLabel({id:"labelTelefono",height:"auto",width:"70%",text:L("telephone"),font:{fontSize:"16dp"},color:e,textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT}),textTelefono=Ti.UI.createTextArea({id:"textTelefono",value:a.fieldByName("phone"),height:Ti.UI.SIZE,width:"70%",font:{fontSize:"16dp"},color:i,enabled:"false"}),labelDireccion=Ti.UI.createLabel({id:"labelDireccion",height:"auto",width:"70%",text:L("address"),font:{fontSize:"16dp"},color:e,textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT}),textDireccion=Ti.UI.createTextArea({id:"textDireccion",value:a.fieldByName("address"),height:Ti.UI.SIZE,width:"70%",font:{fontSize:"16dp"},color:i,enabled:"false"}),labelGrupo=Ti.UI.createLabel({id:"labelGrupo",height:"auto",width:"70%",text:L("grupo"),font:{fontSize:"16dp"},color:e,textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT}),textGrupo=Ti.UI.createTextArea({id:"textSubgrupo",value:a.fieldByName("group_name"),height:Ti.UI.SIZE,width:"70%",font:{fontSize:"16dp"},color:i,enabled:"false"}),labelSubgrupo=Ti.UI.createLabel({id:"labelSubgrupo",height:"auto",width:"70%",text:L("subgrupo"),font:{fontSize:"16dp"},color:e,textAlign:Ti.UI.TEXT_ALIGNMENT_LEFT}),textSubgrupo=Ti.UI.createTextArea({id:"textSubgrupo",value:a.fieldByName("subgroup_name"),height:Ti.UI.SIZE,width:"70%",font:{fontSize:"16dp"},color:i,enabled:"false"}),buttonSugerencias=Ti.UI.createButton({id:"buttonSugerencias",height:"auto",width:"70%",title:L("suggestions"),font:{fontSize:"18dp"},color:"blue",top:"10px",systemButton:Ti.UI.iPhone.SystemButton.CANCEL}),t.close(),a.close(),scrollView.add(labelEmpresa),scrollView.add(textEmpresa),scrollView.add(labelTelefono),scrollView.add(textTelefono),scrollView.add(labelDireccion),scrollView.add(textDireccion),scrollView.add(labelGrupo),scrollView.add(textGrupo),scrollView.add(labelSubgrupo),scrollView.add(textSubgrupo),scrollView.add(buttonSugerencias),imageViewBar.add(imageView),imageViewBar.add(labelTitulo),imageViewBar.add(buttonClose),registroWdw.add(imageViewBar),registroWdw.add(scrollView),buttonSugerencias.addEventListener("click",function(){var e=Ti.Database.open("anadicDB");e.execute("CREATE TABLE IF NOT EXISTS rating(rateId INTEGER PRIMARY KEY, rate TEXT);");var i=e.execute("SELECT rate FROM rating WHERE rateId = 1;");if(e.close(),i.isValidRow())alert(L("qualified")+"\n"+L("qualified2")+" "+i.fieldByName("rate")),i.close();else{i.close();var t=require("/ui/common/ContactoWindow");new t.ContactoWindow}}),buttonClose.addEventListener("click",function(){Ti.Media.vibrate(),registroWdw.close()}),registroWdw.addEventListener("android:back",function(){Ti.Media.vibrate(),registroWdw.close()}),registroWdw}module.exports=RegistroWindow;