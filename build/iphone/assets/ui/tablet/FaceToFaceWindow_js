function FaceToFaceWindow(){function e(){Ti.Media.vibrate(),faceToFaceWdw.close()}function i(){var e=[],i=Ti.UI.createTableViewRow({id:2,title:L("schedules"),leftImage:"/images/horarios.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i);var i=Ti.UI.createTableViewRow({id:3,title:L("map"),leftImage:"/images/mapa.png",isparent:!0,opened:!1,hasChild:!1,font:{fontSize:"22dp"},color:"black"});e.push(i),table.setData(e)}var t=require("lib/network"),o=require("tools"),a=o.isiOS7Plus();faceToFaceWdw=Ti.UI.createWindow({tabBarHidden:!0,backgroundColor:"white",width:"100%",height:"100%",layout:"vertical",fullscreen:a,navBarHidden:!0}),table=Ti.UI.createTableView({width:"90%",height:"100%"}),scrollView_1=Ti.UI.createView({id:"scrollView_1",backgroundImage:"/images/background.png",height:"100%",width:"100%",layout:"vertical"}),scrollView_1.add(table);var n=require("templates"),r=n.getTopBar(L("facetoface"),"/images/iconfacetoface.png",e);return faceToFaceWdw.add(r),faceToFaceWdw.add(scrollView_1),i(),table.addEventListener("click",function(e){if(2==e.rowData.id)t.getFaceToFaceDays(function(e){if(0==e.length)Ti.UI.createAlertDialog({message:L("no_appoinments"),ok:L("ok"),title:L("alert_title")}).show();else if(e.length>0){var i,t=require("ui/handheld/facetoface/HorariosWindow");new t(i,e).open()}});else if(3==e.rowData.id){var i,o=require("ui/handheld/mapa/MapaWindow");new o(i).open()}}),faceToFaceWdw.addEventListener("android:back",function(){Ti.Media.vibrate(),faceToFaceWdw.close()}),faceToFaceWdw}module.exports=FaceToFaceWindow;